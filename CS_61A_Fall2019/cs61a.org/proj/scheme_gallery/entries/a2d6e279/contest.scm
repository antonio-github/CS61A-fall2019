(define (is-this-tail-recursive time lyrics)
	(define memetime (modulo time 100))
	(my-eyes-burn)
	(if (= 1 (modulo time 10))
		(begin (clear) (eecs time) (rickroll lyrics) (ooh-an-event-flag))
		0
	)
	(if (and (< memetime 51) (> memetime 48))
		(begin (clear) (rickroll lyrics) (onii-eecs time) (ooh-an-event-flag))
		0
	)
	(if (and (< memetime 35) (> memetime 15) (= 2 (modulo time 5)))
		(give-me-cs-degree)
		0
	)
	(if (and (> memetime 65) (< memetime 85) (= 2 (modulo time 5)))
		(aNounceMents)
		0
	)
	(if (< time 1100)
		(if (= 5 (modulo time 20))
			(is-this-tail-recursive (+ time 1) (cdr lyrics))
			(is-this-tail-recursive (+ time 1) lyrics)
		)
		(exitonclick)
	)
)


